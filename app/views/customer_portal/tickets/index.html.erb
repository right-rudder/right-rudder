<div class="lg:pl-72">
  <div class="p-10">
    <h1 class="font-bold text-5xl">All Squawks</h1>
  </div>

  <%= render partial: "shared/tabs", locals: { all_squawks_path: tickets_path, my_squawks_path: tickets_path(filter: "my-tickets") }  %>

  <%= turbo_frame_tag "tickets_frame" do %>
    <div class="px-10">
      <% @accounts.each do |account| %>
        <%= render partial: "tickets/account_tickets", locals: { account: account, source: "portal_index" } %>
      <% end %>
    </div>
  <% end %>

</div>

